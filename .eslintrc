{
  "plugins": [
    "no-loops",
    "prettier"
  ],
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "rules": {
    "no-console": "off",               // Whether block console.log statement.
    "no-loops/no-loops": "error",      // use map / forEach instead of for / while
    "prettier/prettier": "error"
  }
}
